{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@components/*": ["./src/components/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@pages/*": ["./src/pages/*"],
      "@public/*": ["./public/*"],
      "@styles/*": ["./src/styles/*"],
      "@utils/*": ["./src/utils/*"],
      "@scripts/*": ["./src/scripts/*"],
      "@data/*": ["./src/data/*"],
      "@images/*": ["./src/assets/images/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "react"
  }
}
